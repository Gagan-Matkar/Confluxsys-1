     <bean name = "Test">
		<property name="name" value="radhe"></property>
		<property name="phones">
			<list>
				<value>123456789</value>
				<value>987456321</value>

				<value>147258369</value>




			</list>
		</property>
		<property name="address">
			<set>
				<value>pune</value>
				<value>pimpri</value>
				<value>mumbai</value>


			</set>


		</property>

		<property name="courses">
			<map>
				<entry key= "java" value = "2mon" />
				<entry key= "python" value = "1.2mon"/>
				

				<entry key = "c" value = "1mon" />

			</map>

		</property>


	</bean>

